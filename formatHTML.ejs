<!DOCTYPE html>
<html lang="en">
<head>
  <style>
    .tableSetting {
      border: black 2px solid;
      border-spacing: 0;
    }
    .bodySetting {
      border: red 1px solid;
      /* border-spacing: 0; */
    }
  </style>
</head>

<body>

    <% for(const itemArray of list) { %>
      <table class="tableSetting">
        <thead>
          <tr>
            <th colspan="3" class="bodySetting" ><h2><%= itemArray.name %></h2></th>
            <th colspan="3" class="bodySetting" ><h2>Before</h2></th>
            <th colspan="3" class="bodySetting" ><h2>Now</h2> </th>
          </tr>
        </thead>
        
        <tbody>
          <tr>
            <% for(const objk in itemArray) { %>
              <% if (objk !== "name") { %>
                <tr>
                    <td colspan="3" class="bodySetting">
                      <h3>Item: <%= Number(objk) + 1 %></h3>
                      <h3>PID: <%= itemArray[objk].pid %></h3>
                    </td>
                    <td colspan="3" class="bodySetting"></td>
                    <td colspan="3" class="bodySetting">
                      <h3><a href="<%= itemArray[objk].url %>"><%= itemArray[objk].title %> </a></h3>
                      <h3> Price: <%= itemArray[objk].price %> </h3>
                    </td>
                  </tr>
              <% } %>
            <% } %>
          </tr>
        </tbody>
      </table>
      <br>
    <% } %>

</body>
</html>